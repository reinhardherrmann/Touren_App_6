<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.orome.tourenapp6">

    <application
        android:name=".application.TourenApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TourenApp6">
        <activity android:name=".view.touren.NewTourActivity"></activity>
        <activity
            android:name=".view.touren.TourenActivity"
            android:label="@string/title_activity_touren" />
        <activity android:name=".view.home.HomeActivity" />
        <activity
            android:name=".view.splash.SplashActivity"
            android:theme="@style/Theme.TourenApp6.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>